package pt.ulisboa.tecnico.softeng.bank.domain;

class Bank {
	String name;
	String code;
}

relation DomainRootHasBankApplication {
	.pt.ist.fenixframework.DomainRoot playsRole root {
		multiplicity 1..1;
	}
	Bank playsRole bank {
		multiplicity 0..*;
	}
}

class Account {
	String IBAN;
	int balance;
}

class Client {
	String name;
	String ID;
}

class Operation{
	String reference;
	int value;
	DateTime time;
}
